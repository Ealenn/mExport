{
  "name": "mexport",
  "version": "0.1.0",
  "description": "Mail Export - Clean Mail Inbox - Unsubscribe spam email - View Accounts",
  "main": "src/Main.ts",
  "directories": {
    "dist": "dist"
  },
  "bin": {
    "mexport": "./dist/Main.js"
  },
  "scripts": {
    "build": "tsc -p .",
    "build:watch": "rm -rf ./dist && tsc -p . -w",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "test": "jest --coverage",
    "local": "npm run build && npm i -g && mexport",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "docker:run": "docker run --rm mexport:dev",
    "docker:build": "docker buildx build . -t mexport:dev"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/tests/.*|\\.(spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "compiler": "ttypescript"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ealenn/mExport.git"
  },
  "keywords": [
    "mexport",
    "email",
    "inbox",
    "clean",
    "spam",
    "smtp",
    "mailbox"
  ],
  "author": "Rudy Marchandise",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ealenn/mExport/issues"
  },
  "homepage": "https://github.com/Ealenn/mExport#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^6.2.0",
    "figlet": "^1.5.0",
    "imap-simple": "^5.0.0",
    "lodash": "^4.17.21",
    "mailparser": "^3.2.0",
    "prompts": "^2.4.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.0",
    "tsyringe": "^4.4.0",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/chai": "^4.2.14",
    "@types/figlet": "^1.2.1",
    "@types/imap-simple": "^4.2.4",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.170",
    "@types/mailparser": "^3.0.2",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.10",
    "@types/prompts": "^2.0.9",
    "@types/sinon": "^9.0.9",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "chai": "^4.2.0",
    "eslint": "^7.29.0",
    "jest": "^26.6.3",
    "mocha": "^8.2.1",
    "moq.ts": "^6.4.0",
    "nodemon": "^2.0.6",
    "sinon": "^9.2.1",
    "ts-jest": "^26.4.4",
    "ts-mock-imports": "^1.3.1",
    "ts-node": "^9.0.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.1.2"
  }
}
